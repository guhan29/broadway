<div class="col-md-10 col-md-offset-1">
  <div class="edit-review-form">
    <h1>Edit Review<h1/>
    <%= simple_form_for [@play, @review] do |f| %>
      <div id="rating-form">
        <%= f.input :rating, input_html: { max: 5 } %>
        <%= f.input :comment %>
        <%= f.button :submit, class: "btn-custom2" %>
      </div>
    <% end %>
  </div>
</div>